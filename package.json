{
  "name": "overpass-proxy",
  "version": "1.0.0",
  "description": "Amenity-focused Overpass API proxy with Redis tile caching.",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "lint": "eslint \"src/**/*.ts\"",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "test": "cross-env USE_DOCKER=0 vitest run",
    "test:watch": "cross-env USE_DOCKER=0 vitest",
    "test:docker": "cross-env USE_DOCKER=1 vitest run",
    "test:ci": "cross-env USE_DOCKER=0 vitest run --coverage"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@fastify/formbody": "^7.4.0",
    "fastify": "^4.26.2",
    "fastify-plugin": "^4.5.1",
    "got": "^12.6.0",
    "ioredis": "^5.3.2",
    "ngeohash": "^0.6.0",
    "pino": "9.14.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/ngeohash": "^0.6.8",
    "@types/node": "^20.11.30",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^8.12.2",
    "@typescript-eslint/parser": "^8.12.2",
    "cross-env": "^10.1.0",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "globals": "^16.4.0",
    "supertest": "^6.3.3",
    "testcontainers": "^10.7.1",
    "tsx": "^4.7.1",
    "typescript": "^5.4.5",
    "vitest": "^3.2.4"
  }
}
